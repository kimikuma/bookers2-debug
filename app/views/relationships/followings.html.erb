<div class="container">
  <div class="row">
    <div class="col-12">
<h1>Follow Users</h1>
<% if @users.exists? %>
<% @users.each do |user| %>
<table class="table table_border_top p-10">
  <tr>
    <th>image</th>
    <th>name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
  <tr>
    <td><%=image_tag user.get_profile_image, size: '50x50' %></td>
    <td><%=user.name %></td>
    <td>フォロー数:<%=user.followings.count %></td>
    <td>フォロワー数:<%=user.followers.count %></td>
    <td><%=link_to "Show",user_path(user) %></td>
  </tr>
  <% end %>
</table>
<% else %>
<p>ユーザーはいません</p>
<% end %>
</div>
</div>
</div>